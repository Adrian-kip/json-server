const axios = require('axios');

async function deleteBootcamp(userId) {
  try {
    const url = `https://bootcamps-api-19sb.onrender.com/bootcamps/${userId}`;
    const response = await axios.delete(url);

    if (response.status === 200) {
      console.log(`Bootcamp with ID ${userId} deleted successfully.`);
    } else {
      console.log(`Failed to delete bootcamp with ID ${userId}. Status code: ${response.status}`);
    }
  } catch (error) {
    console.error('Error deleting bootcamp:', error.message);
  }
}
deleteBootcamp(1,2,3,4,5,6,1743066642570);